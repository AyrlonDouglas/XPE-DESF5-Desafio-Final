{
	"name": "xpe-desf5-desafio-final",
	"version": "1.0.0",
	"description": "Atividade avaliativa do módulo 5 da pós-graduação em Arquitetura de software e soluções",
	"main": "src/main.js",
	"engines": {
		"node": "22.18.0",
		"npm": "10.9.3"
	},
	"scripts": {
		"dev": "tsx watch --env-file .env src/main.ts",
		"type": "tsc --noEmit --watch",
		"type-check": "tsc --noEmit",
		"db:generate": "drizzle-kit generate",
		"db:migrate": "drizzle-kit migrate",
		"build": "rm -rf dist && tsc && tsc-alias",
		"up": "docker compose up -d --build --remove-orphans && docker compose logs -f api",
		"stop": "docker compose stop",
		"down-v": "docker compose down -v"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/AyrlonDouglas/XPE-DESF5-Desafio-Final.git"
	},
	"author": "",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/AyrlonDouglas/XPE-DESF5-Desafio-Final/issues"
	},
	"homepage": "https://github.com/AyrlonDouglas/XPE-DESF5-Desafio-Final#readme",
	"dependencies": {
		"drizzle-orm": "^0.44.4",
		"drizzle-seed": "^0.3.1",
		"express": "^5.1.0",
		"pg": "^8.16.3"
	},
	"devDependencies": {
		"@tsconfig/node-lts": "^22.0.2",
		"@types/express": "^5.0.3",
		"@types/node": "^24.3.0",
		"@types/pg": "^8.15.5",
		"drizzle-kit": "^0.31.4",
		"tsc-alias": "^1.8.16",
		"tsx": "^4.20.4",
		"typescript": "^5.9.2"
	}
}
